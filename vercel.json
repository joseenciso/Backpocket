{
    "builds": [
      {
        "src": "ms4ecommerce/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "runtime": "python3.9",
          "maxLambdaSize": "15mb"
        },
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
            "disDir": "staticfiles_build"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "ms4ecommerce/wsgi.py"
      }
    ]
  }